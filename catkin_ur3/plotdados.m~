function plotdados(x)
clear all;
figure;
T = readtable('dados.csv');
dados = table2array(T);
subplot(1,3,1);
plot(dados(:,1),dados(:,x+2),'r');
ylim([-1.5 1.5]);
title('pj0');
grid on;
%%%%%%%%%%%%%%%%;%%%%%%%%%%%%%%%%%%
subplot(1,3,2);
plot(dados(:,1),dados(:,x+3),'g');
ylim([-0.6 0.6]);
title('vj0');
grid on ;
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
subplot(1,3,3);
plot(dados(:,1),dados(:,x+4),'b');
ylim([-2.5 2.5]);
title('tj0')
grid on; 
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
clear all;